<p id="notice"><%= notice %></p>

<h1>Warehousetypes</h1>

<table>
  <thead>
    <tr>
      <th>Type item</th>
      <th>Warehouse</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warehousetypes.each do |warehousetype| %>
      <tr>
        <td><%= warehousetype.type_item %></td>
        <td><%= warehousetype.warehouse %></td>
        <td><%= link_to 'Show', warehousetype %></td>
        <td><%= link_to 'Edit', edit_warehousetype_path(warehousetype) %></td>
        <td><%= link_to 'Destroy', warehousetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warehousetype', new_warehousetype_path %>
